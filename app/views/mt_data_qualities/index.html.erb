<% @mt_data_qualities.each do |mt_data_quality| %>
  <tr>
    <td><%= mt_data_quality.mt_product_id %></td>
    <td><%= mt_data_quality.materials %></td>
    <td><%= mt_data_quality.manufacturing %></td>
    <td><%= mt_data_quality.distribution %></td>
    <td><%= mt_data_quality.use %></td>
    <td><%= mt_data_quality.end_of_life %></td>
    <td><%= link_to 'Show', mt_data_quality %></td>
    <td><%= link_to 'Edit', edit_mt_data_quality_path(mt_data_quality) %></td>
    <td><%= link_to 'Destroy', mt_data_quality, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

