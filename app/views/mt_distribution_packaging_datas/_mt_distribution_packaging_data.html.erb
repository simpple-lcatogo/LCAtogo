<tr id="mt_distribution_packaging_data_<%= mt_distribution_packaging_data.id %>">
     <td>
	      <% if mt_distribution_packaging_data.mt_material  %>
			<%= mt_distribution_packaging_data.mt_material.name.html_safe %>
	      <% end %>		
     </td>
     <td class="quantity_<%= mt_distribution_packaging_data.id %>">
          <%= mt_distribution_packaging_data.quantity %>
     </td>
    <script type="text/javascript">
		   $(".quantity_<%=mt_distribution_packaging_data.id%>").editInPlace({
		        url: "/mt_distribution_packaging_datas/update_values" + "?&authenticity_token=" + encodeURIComponent(AUTH_TOKEN),
		        params: "mt_distribution_packaging_data_id=<%=mt_distribution_packaging_data.id%>",
		        saving_text: '<%=_("Saving...")%>'
		        });      
  
     </script>        
     <td>
          <%=_('kg').html_safe%>
     </td>     
     <td>
     	<% if mt_distribution_packaging_data.scenario == 'recycling' %>
          <%=_('Recycling').html_safe%>
		<% end %>  
		<% if mt_distribution_packaging_data.scenario == 'incineration' %>
		  <%=_('Incineration').html_safe%>
		<% end %>  
		<% if mt_distribution_packaging_data.scenario == 'landfill' %> 
	 	  <%=_('Landfill').html_safe%>		
		<% end %> 
     </td>	 
     <td class="center"><%= link_to '&#10006;'.html_safe, mt_distribution_packaging_data_path(mt_distribution_packaging_data, :authenticity_token => form_authenticity_token), :method => :delete, :class => "delete", :remote => true  %></td>
</tr>
