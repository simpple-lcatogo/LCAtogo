<tr id="mt_end_of_life_data_<%= mt_end_of_life_data.id %>">
     <td>
	      <% if mt_end_of_life_data.mt_material  %>
			<%= mt_end_of_life_data.mt_material.name.html_safe %>
	      <% end %>		
     </td>
     <td>
          <%= mt_end_of_life_data.quantity %>
     </td>
     <td>
          <%=_('km').html_safe%>
     </td>     
     <td>
     	<% if mt_end_of_life_data.scenario == 'recycling' %>
          <%=_('Recycling').html_safe%>
		<% end %>  
		<% if mt_end_of_life_data.scenario == 'incineration' %>
		  <%=_('Incineration').html_safe%>
		<% end %>  
		<% if mt_end_of_life_data.scenario == 'landfill' %> 
	 	  <%=_('Landfill').html_safe%>		
		<% end %> 
     </td>	 
     <td class="center"><%= link_to '&#10006;'.html_safe, mt_end_of_life_data_path(mt_end_of_life_data, :authenticity_token => form_authenticity_token), :method => :delete, :class => "delete", :remote => true  %></td>
</tr>
