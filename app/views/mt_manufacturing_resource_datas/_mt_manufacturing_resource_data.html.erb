<tr id="mt_manufacturing_resource_data_<%= mt_manufacturing_resource_data.id %>">
     <td>
	      <% if mt_manufacturing_resource_data.mt_consumable %>
			<%= mt_manufacturing_resource_data.mt_consumable.name.html_safe %>
	      <% end %>		
     </td>
     <td class="quantity_<%=mt_manufacturing_resource_data.id%>">
          <%= mt_manufacturing_resource_data.quantity %>
     </td>
     <script type="text/javascript">
		   $(".quantity_<%=mt_manufacturing_resource_data.id%>").editInPlace({
		        url: "/mt_manufacturing_resource_datas/update_values" + "?&authenticity_token=" + encodeURIComponent(AUTH_TOKEN),
		        params: "mt_manufacturing_resource_data_id=<%=mt_manufacturing_resource_data.id%>",
		        saving_text: '<%=_("Saving...")%>'
		        });      
  
     </script>              
     <td>
          <%=_('kg').html_safe%>
     </td>     
     <td class="center"><%= link_to '&#10006;'.html_safe, mt_manufacturing_resource_data_path(mt_manufacturing_resource_data, :authenticity_token => form_authenticity_token), :method => :delete, :class => "delete", :remote => true  %></td>
</tr>
