<tr id="mt_waste_energy_data_<%= mt_waste_energy_data.id %>">
     <td>
	      <% if mt_waste_energy_data.mt_family %>
			<%= mt_waste_energy_data.mt_family.name.html_safe %>
	      <% end %>		
     </td>
     <td>
	      <% if mt_waste_energy_data.mt_material %>
			<%= mt_waste_energy_data.mt_material.name.html_safe %>
	      <% end %>		

     </td>
	 <% if mt_waste_energy_data.mt_material %>
	   <% if mt_waste_energy_data.mt_material.unit == 'kg'%>     
          <td id="td_quantity_material_<%= mt_waste_energy_data.id %>" class="quantity_<%= mt_waste_energy_data.id %>">
       <% else %>
          <td class="quantity_<%= mt_waste_energy_data.id %>">
       <% end %>
     <% else %>
       <td class="quantity_<%= mt_waste_energy_data.id %>">
     <% end %>
          <%= mt_waste_energy_data.quantity %>
     </td>
     <script type="text/javascript">
		   $(".quantity_<%=mt_waste_energy_data.id%>").editInPlace({
		        url: "/mt_waste_energy_datas/update_values" + "?&authenticity_token=" + encodeURIComponent(AUTH_TOKEN),
		        params: "mt_waste_energy_data_id=<%=mt_waste_energy_data.id%>",
		        saving_text: '<%=_("Saving...")%>',
		        success: function(msg) {updateWeightDetailed();}
		        });      
  
     </script>            
     
     <td>
	      <% if mt_waste_energy_data.mt_material %>
			<%= mt_waste_energy_data.mt_material.unit.html_safe %>
	      <% end %>		
     </td>     
     <td class="center"><%= link_to '&#10006;'.html_safe, mt_waste_energy_data_path(mt_waste_energy_data, :authenticity_token => form_authenticity_token), :method => :delete, :class => "delete", :remote => true  %></td>
</tr>
