<h1><%=_('Data Quality').html_safe%>
</h1>
<div class="content_skin">
	<div class="lcamenu span-23 showdgrid last ">
	  <%= render(:partial => 'shared/se_data_qualities/tabs') %>
	</div>
	<div class="clear"></div>
<%= form_for(@se_data_quality) do |f| %>
  <table class="standard2">
   <tbody>
    <tr>
      <th colspan="4">&nbsp;</th>
    </tr>
    <tr>
      <td><%=_('SENSOR LIFE CYCLE').html_safe%></td>
	  <td><%= f.radio_button :life_cycle, "robust" %>  <%=_('Robust').html_safe%></td>
	  <td><%= f.radio_button :life_cycle, "indicative" %>  <%=_('Indicative').html_safe%></td>
	  <td><%= f.radio_button :life_cycle, "illustrative" %>  <%=_('Illustrative').html_safe%></td>	  
	</tr>
	<tr>
      <td><%=_('PRODUCTION - ENERGY USAGE').html_safe%></td>
      <td><%= f.radio_button :energy_usage, "robust" %>  <%=_('Robust').html_safe%></td>
      <td><%= f.radio_button :energy_usage, "indicative" %>  <%=_('Indicative').html_safe%></td>
      <td><%= f.radio_button :energy_usage, "illustrative" %>  <%=_('Illustrative').html_safe%></td>  
	</tr>
	<tr>
      <td><%=_('PRODUCTION - CONSUMABLES USAGE').html_safe%></td>
      <td><%= f.radio_button :consumables_usage, "robust" %>  <%=_('Robust').html_safe%></td>
      <td><%= f.radio_button :consumables_usage, "indicative" %>  <%=_('Indicative').html_safe%></td>
      <td><%= f.radio_button :consumables_usage, "illustrative" %>  <%=_('Illustrative').html_safe%></td>	  
	</tr>
	<tr>
      <td><%=_('PRODUCTION - UTILITIES USAGE').html_safe%></td>
      <td><%= f.radio_button :utilities_usage, "robust" %>  <%=_('Robust').html_safe%></td>
      <td><%= f.radio_button :utilities_usage, "indicative" %>  <%=_('Indicative').html_safe%></td>
      <td><%= f.radio_button :utilities_usage, "illustrative" %>  <%=_('Illustrative').html_safe%></td>	  
	</tr>
	<tr>
      <td><%=_('YIELD LOSS').html_safe%></td>
      <td><%= f.radio_button :yield_loss, "robust" %>  <%=_('Robust').html_safe%></td>
      <td><%= f.radio_button :yield_loss, "indicative" %>  <%=_('Indicative').html_safe%></td>
      <td><%= f.radio_button :yield_loss, "illustrative" %>  <%=_('Illustrative').html_safe%></td>	  
	</tr>
	
	</tbody> 
   </table>
   
  <div class="buttons last" style="float:left">
     <a href="<%=se_data_quality_intro_path(@se_product)%>" class="positive"><img alt="Tick" src="/images/tick.png"><%= _("Previous step").html_safe %></a>
  </div>
   
   
  <div class="buttons" style="float:right">
     <button type="submit" class="positive"> <%= image_tag 'tick.png' %><%= _("Data Update").html_safe %></button>
   </div>

  <div class="clear"></div>
  <div class="navbar">
    <!-- <div id="for-ie"> -->
    <div>
      <ul>
        <li><%= link_to _('To Introduction').html_safe, se_production_intro_path(@product.se_product) %></li>
        <li><%= link_to _('To Data Entry').html_safe, se_manufacturing_path(@product.se_product) %></li>
        <li><%= link_to _('To Results').html_safe, se_result_path(@product.se_product) %></li>
      </ul>
    </div>
  </div>
  <div class="clear"></div>


</div>

	
	
<% end %>

</div>	