<tr id="se_use_product_data_<%= se_use_product_data.id %>">
  <% if defined?(page) && page == 'use_cost' %>
      
      <td><%= se_use_product_data.output_description%></td>
      <td style="text-align:center">
        <span class="with_sensor">
          <% if !se_use_product_data.share_with_sensor.nil? %>
              <%= number_with_precision(se_use_product_data.share_with_sensor, :precision => 2, :locale => 'en') %>
          <% else %> 
              - 
          <% end %>
        </span>
        
        <span>%</span>
      </td>

      <td style="text-align:center">
        <span class="without_sensor">  
          <% if !se_use_product_data.share_without_sensor.nil? %>
             <%= number_with_precision(se_use_product_data.share_without_sensor, :precision => 2, :locale => 'en')%>
          <% else %>
              -
          <% end %>
        </span>
        
        <span>%</span>
      </td>

      <td class="center" id="se_use_product_data_cost_<%=se_use_product_data.id%>"><%= se_use_product_data.cost %><%= ' &euro;/kg'.html_safe if se_use_product_data.cost %></td>

       <script type="text/javascript">
         $("#se_use_product_data_cost_<%=se_use_product_data.id%>").editInPlace({
              url: "/se_use_product_datas/update_values" + "?&authenticity_token=" + encodeURIComponent(AUTH_TOKEN),
              params: "id=<%=se_use_product_data.id%>&field=cost",
              saving_text: '<%=_("Saving...")%>',
              default_text: '<%= _("(value)")%>'
              });
      </script> 
    

    <% else %>

      <td><%= se_use_product_data.output_description%></td>
      <td style="text-align:center">
        <span class="with_sensor">
          <% if !se_use_product_data.share_with_sensor.nil? %>
              <%= number_with_precision(se_use_product_data.share_with_sensor, :precision => 2, :locale => 'en') %>
          <% else %> 
              - 
          <% end %>
        </span>
        
        <span>%</span>
      </td>

      <td style="text-align:center">
        <span class="without_sensor">  
          <% if !se_use_product_data.share_without_sensor.nil? %>
             <%= number_with_precision(se_use_product_data.share_without_sensor, :precision => 2, :locale => 'en')%>
          <% else %>
              -
          <% end %>
        </span>
        
        <span>%</span>
      </td>

      <td class="center"><%= link_to '&#10006;'.html_safe, se_use_product_data_path(se_use_product_data, :authenticity_token => form_authenticity_token), :method => :delete, :class => "delete", :remote => true  %></td>
    

   <% end %>

  </tr>