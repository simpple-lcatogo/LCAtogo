<ul>
  <% if @product.id %>
     <li><%= tab_link_to _('Detailed Results').html_safe, product_result_url(@product), :product => @product, :index => 1 %></li>
     <li style="padding-top:10px;padding-bottom:10px"> > </li>
     <li><%= tab_link_to _('Graphic Results').html_safe, product_result_graphic_path(@product), :product => @product, :index => 2 %></li>     
     <li style="padding-top:10px;padding-bottom:10px"> > </li>
     <li><%= tab_link_to _('Comparison').html_safe, comparison_url(@product), :product => @product, :index => 3 %></li>
   <% end %>
<div class="clear"></div>
</ul>
